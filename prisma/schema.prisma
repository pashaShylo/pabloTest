generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
  binaryTargets = ["libquery_engine-debian-openssl-1.1.x.so.node","schema-engine-debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") 
  directUrl = env("POSTGRES_URL_NON_POOLING") 
}

model UserResult{
  id Int @id @default(autoincrement())
  userName String
  result Json[]
}
